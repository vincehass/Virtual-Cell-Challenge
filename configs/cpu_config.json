{
    "data": {
        "max_cells": 10000,
        "max_genes": 2000,
        "min_cells_per_batch": 20
    },
    "model": {
        "se_config": {
            "embed_dim": 256,
            "n_heads": 8,
            "n_layers": 6,
            "dropout": 0.1
        },
        "st_config": {
            "state_dim": 128,
            "perturbation_dim": 128,
            "n_heads": 4,
            "n_layers": 3,
            "dropout": 0.1
        }
    },
    "training": {
        "epochs": 100,
        "batch_size": 32,
        "learning_rate": 1e-4,
        "weight_decay": 1e-4
    },
    "evaluation": {
        "sample_size": 500,
        "bootstrap_samples": 500
    }
}
